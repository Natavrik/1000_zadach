                    printf("введите 2 числа и выведите порядковый номер меньшего из них:  ");
                    scanf("%d%d", &a,&b);
                    if (a<b)
                    printf(" Ваш ответ: %d", o);
                    else
                    printf(" Ваш ответ: %d", d);
                    
    return 0;
}
